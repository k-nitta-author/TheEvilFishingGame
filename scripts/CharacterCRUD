# static class for handling CRUD operations on Dialog Data
class Character:
    
    def create() -> str:
        pass

    def read() -> str:
        pass

    def update() -> str:
        pass

    def delete() -> str:
        pass

def parse_input(i: int):

    commands_dict = {
        1: Character.create,
        2: Character.read,
        3: Character.update,
        4: Character.delete,
        9: exit
    }

    return commands_dict[i]

